{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false
  },
  "include": [
    "next-env.d.ts",
    "pages/**/*.ts",
    "pages/**/*.tsx",
    "psrcages/**/*.ts",
    "src/**/*.tsx",
    ".eslintrc.js",
    "next.config.js",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "exclude": ["node_modules", "public"]
}
